      <pou name="CounterST" pouType="functionBlock">
        <interface>
          <inputVars>
            <variable name="Reset">
              <type>
                <BOOL/>
              </type>
            </variable>
          </inputVars>
          <localVars>
            <variable name="Cnt">
              <type>
                <INT/>
              </type>
            </variable>
          </localVars>
          <outputVars>
            <variable name="OUT">
              <type>
                <INT/>
              </type>
            </variable>
          </outputVars>
          <externalVars constant="true">
            <variable name="ResetCounterValue">
              <type>
                <INT/>
              </type>
            </variable>
          </externalVars>
        </interface>
        <body>
          <ST>
            <xhtml:p><![CDATA[IF Reset THEN
  Cnt := ResetCounterValue;
ELSE
  Cnt := Cnt + 1;
END_IF;

Out := Cnt;]]></xhtml:p>
          </ST>
        </body>
      </pou>
